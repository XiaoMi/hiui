{
  "name": "@hi-ui/hiui",
  "version": "1.3.0",
  "description": "HIUI for React",
  "scripts": {
    "test": "node_modules/.bin/standard && node_modules/.bin/stylelint --config .stylelintrc 'components/**/*.scss'",
    "fix": "node_modules/.bin/standard --fix && node_modules/.bin/stylelint --config .stylelintrc 'components/**/*.scss' --fix",
    "precommit": "lint-staged",
    "compile": "cross-env NODE_ENV=production gulp",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack --progress --profile --colors --config ./build/webpack.production.config.js && gulp carry",
    "deploy": "gh-pages -d dist",
    "dev": "cd site && cross-env NODE_ENV=development PORT=4200 node server.js",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:XiaoMi/hiui.git"
  },
  "keywords": [
    "component",
    "design",
    "react",
    "react component",
    "template",
    "ui"
  ],
  "author": "HIUI <mi-hiui@xiaomi.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "es/index.js",
  "file": [
    "dist",
    "es"
  ],
  "dependencies": {
    "@hi-ui/classic-theme": "^0.9.6",
    "@hi-ui/core-css": "^1.0.0",
    "async-validator": "^1.8.5",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "component-classes": "^1.2.6",
    "date-fns": "^1.29.0",
    "hoist-non-react-statics": "^2.5.0",
    "lodash": "^4.17.10",
    "mini-store": "^1.1.0",
    "omit.js": "^1.0.0",
    "rc-util": "^4.5.0",
    "react": "^16.3.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-click-outside": "^3.0.1",
    "react-dom": "^16.3.2",
    "react-lifecycles-compat": "^3.0.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "shallowequal": "^1.0.2",
    "universal-observer": "^1.0.1",
    "warning": "^3.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-standalone": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.5",
    "date-fns": "^1.29.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^7.2.2",
    "marked": "^0.4.0",
    "merge2": "^1.2.2",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.5",
    "prismjs": "^1.14.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "4.2.0",
    "rimraf": "^2.6.2",
    "rucksack-css": "^1.0.2",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0",
    "through2": "^2.0.3",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/es/",
      "/lib/",
      "/dist/",
      "/libs/markdown/code.js",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/standard --fix",
      "git add"
    ]
  }
}
