{
  "name": "@hi-ui/hiui",
  "version": "3.0.0-alpha",
  "description": "HIUI for React",
  "scripts": {
    "test": "node_modules/.bin/standard && node_modules/.bin/stylelint --config .stylelintrc 'components/**/*.scss'",
    "jest": "jest",
    "jest:coverage": "npm run jest -- --coverage",
    "jest:badges": "npm run jest:coverage && jest-coverage-badges --input ./test/coverage/coverage-summary.json --output ./test/button",
    "fix": "node_modules/.bin/standard --fix && node_modules/.bin/stylelint --config .stylelintrc 'components/**/*.scss' --fix",
    "precommit": "lint-staged",
    "compile": "cross-env NODE_ENV=production gulp",
    "build": "node_modules/.bin/webpack --progress --profile --colors --config ./build/webpack.config.prod.js && gulp carry",
    "build:gh-pages": "cross-env DOC_ENV=github node_modules/.bin/webpack --progress --profile --colors --config ./build/webpack.config.prod.js && gulp carry",
    "deploy": "gh-pages -d dist --git \"$(which git)\"",
    "dev": "npm start",
    "start": "node build/start.js",
    "prerender:gh-pages": "cross-env DOC_ENV=github npm run build && cross-env DOC_ENV=github node build/prerenderSite.js",
    "prerender": "npm run build && node build/prerenderSite.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:XiaoMi/hiui.git"
  },
  "keywords": [
    "component",
    "design",
    "react",
    "react component",
    "template",
    "ui"
  ],
  "author": "HIUI <mi-hiui@xiaomi.com>",
  "license": "MIT",
  "main": "index.js",
  "module": "es/index.js",
  "file": [
    "dist",
    "es"
  ],
  "peerDependencies": {
    "react": ">=16.13.0",
    "react-dom": ">=16.13.0"
  },
  "dependencies": {
    "@hi-ui/core-css": "^2.0.1",
    "async-validator": "^1.11.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "date-fns": "^2.2.1",
    "decimal.js": "^10.2.0",
    "fetch-jsonp": "^1.1.3",
    "lodash": "^4.17.11",
    "qs": "^6.8.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-click-outside": "^3.0.1",
    "react-cropper": "1.2.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-lifecycles-compat": "^3.0.4",
    "react-transition-group": "^4.2.1",
    "shallowequal": "^1.0.2",
    "warning": "^3.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/standalone": "^7.4.5",
    "@bougiel/puppeteer-prerenderer": "^0.0.3",
    "@mdx-js/loader": "^1.1.0",
    "autoprefixer": "^8.5.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "canvas": "^2.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "convert-source-map": "^1.6.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "file-loader": "^1.1.11",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "jest-canvas-mock": "2.2.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-fetch-mock": "^2.1.2",
    "jsx-loader": "^0.13.2",
    "lint-staged": "^7.2.2",
    "loader-utils": "^1.2.3",
    "marked": "^0.7.0",
    "merge2": "^1.2.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mockjs": "^1.1.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.1.5",
    "prismjs": "^1.16.0",
    "raw-loader": "^0.5.1",
    "react": "^16.9.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd-test-backend": "^7.7.0",
    "react-dnd-test-utils": "^7.7.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "4.2.0",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rucksack-css": "^1.0.2",
    "run-sequence": "^2.2.1",
    "sass-loader": "^7.0.1",
    "simulant": "^0.2.2",
    "sinon": "^7.3.2",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.7.0",
    "terser-webpack-plugin": "^1.3.0",
    "through2": "^2.0.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "@hi-ui/classic-theme": "1.2.5",
    "@mapbox/rehype-prism": "^0.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "react-live": "^2.1.1",
    "react-redux": "^5.1.1",
    "clipboard": "^2.0.4",
    "github-markdown-css": "^3.0.1",
    "hoist-non-react-statics": "^2.5.0"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "/es/",
      "/lib/",
      "/dist/",
      "/libs/markdown/code.js",
      "/components/**/index.test.js",
      "yarn.lock"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "node_modules/.bin/standard --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --config .stylelintrc  --fix",
      "git add"
    ]
  }
}
